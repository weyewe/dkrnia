 
<tr id="entry-<%= object.id %>">
	<td>
		<%= link_to "#{object.code}" , new_purchase_receival_purchase_receival_entry_url( object)%>
	</td>
	<td>
		<%= "#{object.vendor.name}" %>
	</td>
	
	<td>
		<%= print_date(object.receival_date )%>
	</td>
	 
 	 
	<td>
		<%= object.active_purchase_receival_entries.count %>
	</td>
	
	
	<% if not object.is_confirmed? %>
		<td>
			<%= link_to purchase_receival_url(object.id ), :class =>"delete_entry", :style => "cursor:pointer; text-decoration:none;" do %>
				<i class='icon-remove'></i>Delete
			<% end %>
		</td>

		<td>
			<%= link_to edit_purchase_receival_url(object), 
				:class =>"edit_entry modal-toggler", :style => "cursor:pointer; text-decoration:none;" do %>
				<i class='icon-edit'></i>Edit
			<% end %>
		</td>
	<% else %>
		<td colspan='2'>
			NA
		</td>
	<% end %>
	
</tr>